{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://cipherlock-be.onrender.com/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/health",
            "schedule": "*/10 * * * *"
        }
    ]
}